{
  "name": "bullshit-checkin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:booking": "cd booking && next dev -p 3001",
    "build": "next build && cd functions && npm install && npm run build",
    "build:booking": "cd booking && next build",
    "build:all": "npm run build && npm run build:booking",
    "start": "next start",
    "start:booking": "cd booking && next start -p 3001",
    "lint": "next lint",
    "test": "jest --silent",
    "test:watch": "jest --watch --silent",
    "test:coverage": "jest --coverage --silent",
    "deploy:checkin": "firebase deploy --only hosting:checkin",
    "deploy:booking": "firebase deploy --only hosting:booking",
    "deploy:all": "npm run build:all && firebase deploy"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "firebase": "^11.6.0",
    "googleapis": "^148.0.0",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
